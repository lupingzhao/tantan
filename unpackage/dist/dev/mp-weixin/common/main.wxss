/* 省略号 */
.ellipsis {
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
/* // 第几行显示省略号 */
.ellipsis-3 {
  overflow: hidden;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 3;
}
.ellipsis-2 {
  overflow: hidden;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
}
/* input默认边框的去除方法 */
.input-bk-n {
  outline: none;
}
/* // 删除线 */
.del-line {
  text-decoration: line-through;
}
/* // 用边框画三角形 */
.triangle-topright {
  width: 0;
  height: 0;
  border-top: 200rpx solid red;
  border-left: 300rpx solid transparent;
}
/* /* div类名下的 第2个boxn类名的盒子 *\/ 也可以是 2n 3n 3n+1第几个 之类的 */
.div .boxn:nth-child(2) {
  color: aqua;
}
/* // 让外层盒子撑满屏幕 并且内容居中 */
.div-center {
  position: absolute;
  top: 0;
  right: 0;
  left: 0;
  bottom: 0;
  display: flex;
  justify-content: center;
  align-items: center;
}
.font-break {
  /* 中文字换行 */
  white-space: pre-wrap;
  word-wrap: break-word;
  /* 英文字换行 */
  word-wrap: break-word;
  white-space: normal;
  word-break: break-all;
}
